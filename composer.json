{
    "name": "pragmarx/ci",

    "description": "A Tests Wacher Service & Dashboard Package",

    "keywords": ["laravel", "ci", "continuous integration", "test", "testing", "tdd", "bdd", "watcher"],

    "license": "BSD-3-Clause",

    "authors": [
        {
            "name": "Antonio Carlos Ribeiro",
            "email": "acr@antoniocarlosribeiro.com",
            "role": "Creator & Designer"
        }
    ],

    "require": {
        "php": ">=5.4.0",
        "pragmarx/support": "~0.5",
        "sensiolabs/ansi-to-html": "~1",
        "symfony/process": "^3",
        "cboden/ratchet": "^0.4",
        "jolicode/jolinotif": "^1.2"
    },

    "autoload": {
        "psr-4": {
            "PragmaRX\\TestsWatcher\\": "src/"
        },
        "psr-0": {
            "JasonLewis\\ResourceWatcher": "src/Support/jasonlewis/resource-watcher/src"
        }
    },

    "extra": {
        "component": "package",
        "laravel": {
            "providers": [
                "PragmaRX\\TestsWatcher\\Vendor\\Laravel\\ServiceProvider"
            ]
        }
    },

    "minimum-stability": "dev"
}
